CREATE TABLE message_content (
    message_id TEXT NOT NULL,
    content TEXT NOT NULL,
    type TEXT NOT NULL,
    annotations_json TEXT NOT NULL
);

insertMessageContent:
INSERT INTO message_content(message_id, content, type, annotations_json)
VALUES (?, ?, ?, ?);

selectByMessageId:
SELECT * FROM message_content WHERE message_id = ?;

deleteByMessageId:
DELETE FROM message_content WHERE message_id = ?;

deleteAllMessageContent:
DELETE FROM message_content;